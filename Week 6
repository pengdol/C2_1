struct Circle {
  int x;
  int y;
  double r;
};

int main()
{
  struct Circle c1, c3, c4;
  c1.x = 1; c1.y = 2; c1.r = 1.2;
  printf("원: 좌표(%d %d), 반지름:%1f\n", c1.x, c1.y, c1.r);
  struct Circle c2 = {3, 4, 3.4};
  printf("원: 좌표(%d %d), 반지름:%1f\n", c2.x, c2.y, c2.r);
